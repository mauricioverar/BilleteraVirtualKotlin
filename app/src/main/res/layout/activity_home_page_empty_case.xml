    android:layout_height="match_parent"
    tools:context=".view.HomePageEmptyCase"
    tools:ignore="ContentDescription" >

    <RelativeLayout
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/rectangle_ek12"
            android:layout_width="match_parent"
            android:layout_height="262dp"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:background="@color/soft_blue" />
        <ImageView
            android:id="@+id/path_ek7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:src="@drawable/path_ek7"
            tools:ignore="ImageContrastCheck" />

        <TextView
            android:id="@+id/tvUsername"
            android:layout_width="wrap_content"
            android:layout_height="28.6dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="120dp"
            android:alpha="0.5"
            android:fontFamily="@font/jua"
            android:textColor="@color/white"
            android:textSize="16sp"
            tools:ignore="TextContrastCheck,TextSizeCheck" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="52dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="190dp"
            android:fontFamily="@font/jua"
            android:text="@string/total_balance_number_string"
            android:textColor="@color/white"
            android:textSize="40sp"
            tools:ignore="TextContrastCheck,TextSizeCheck" />

        <TextView
            android:id="@+id/total_balance"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="152dp"
            android:fontFamily="@font/jua"
            android:text="@string/total_balance_string"
            android:textColor="@color/white"
            android:textSize="20sp"
            tools:ignore="TextContrastCheck,TextSizeCheck" />

        <ImageView
            android:id="@+id/profile_picture"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginStart="320dp"
            android:layout_marginTop="48dp"
            android:contentDescription="@string/profile_picture"
            android:src="@drawable/profile_picture"
            tools:ignore="ImageContrastCheck" />
        <ImageView
            android:id="@+id/path_ek8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="335dp"
            android:layout_marginTop="188dp"
            android:src="@drawable/path_ek8"
            tools:ignore="VisualLintBounds,ImageContrastCheck" />

        <TextView
            android:id="@+id/dashboard"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="53dp"
            android:fontFamily="@font/jua"
            android:text="@string/dashboard_string"
            android:textColor="@color/white"
            android:textSize="20sp"
            tools:ignore="TextContrastCheck,TextSizeCheck" />










