    </RelativeLayout>

    <Button
        android:id="@+id/btnSendMoney"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center|center_horizontal"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="8dp"
        android:backgroundTint="@color/bright_green"
        android:drawableLeft="@drawable/path_ek6"
        android:fontFamily="@font/jua"
        android:text="@string/send_money_string"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/btnRequestMoney"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header"
        tools:ignore="RtlHardcoded,SmallSp,TouchTargetSizeCheck,VisualLintButtonSize" />

    <Button
        android:id="@+id/btnRequestMoney"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal|center"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="8dp"
        android:backgroundTint="@color/soft_blue"
        android:drawableLeft="@drawable/path_ek5"
        android:fontFamily="@font/jua"
        android:text="@string/request_money_string"
        android:textColor="@color/white"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnSendMoney"
        app:layout_constraintTop_toBottomOf="@+id/header"
        tools:ignore="RtlHardcoded,SmallSp,TouchTargetSizeCheck,TextContrastCheck,VisualLintButtonSize" />

    <TextView
        android:id="@+id/last_transactions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="228dp"
        android:fontFamily="@font/jua"
        android:text="@string/last_transactions_string"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSendMoney"
        tools:ignore="TextSizeCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="122dp"
        android:layout_marginTop="23dp"
        android:contentDescription="@string/profile_picture"
        android:src="@drawable/empty_illustration"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/last_transactions"
        tools:ignore="ImageContrastCheck" />

    <TextView
        android:id="@+id/there_s_no_transacti"
        android:layout_width="187.2dp"
        android:layout_height="20.2dp"
        android:layout_gravity="center_horizontal|center"
        android:layout_marginStart="98dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="126dp"
        android:layout_marginBottom="130dp"
        android:alpha="0.5"
        android:fontFamily="@font/jua"
        android:gravity="center"
        android:text="@string/there_s_no_transacti_string"
        android:textColor="@color/very_dark_gray"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:ignore="TextContrastCheck,TextSizeCheck" />


